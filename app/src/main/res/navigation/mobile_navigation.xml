<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@id/navigation_home">

    <fragment
        android:id="@+id/navigation_profile"
        android:name="com.example.androidapp.navigation.navigation_profile"
        android:label="fragment_navigation_profile"
        tools:layout="@layout/fragment_navigation_profile" />
    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.androidapp.navigation.navigation_home"
        android:label="fragment_navigation_home"
        tools:layout="@layout/fragment_navigation_home" />
    <fragment
        android:id="@+id/navigation_orders"
        android:name="com.example.androidapp.navigation.navigation_orders"
        android:label="fragment_navigation_orders"
        tools:layout="@layout/fragment_navigation_orders" />
    <fragment
        android:id="@+id/navigation_product"
        android:name="com.example.androidapp.navigation.navigation_product"
        android:label="fragment_navigation_product"
        tools:layout="@layout/fragment_navigation_product" />
    <fragment
        android:id="@+id/navigation_measurements"
        android:name="com.example.androidapp.navigation.navigation_measurements"
        android:label="fragment_navigation_measurements"
        tools:layout="@layout/fragment_navigation_measurements" />
</navigation>