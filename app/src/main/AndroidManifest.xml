<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.androidapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".owner.OwnerHomeActivity"
            android:label="@string/title_activity_owner_home"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".owner.OwnerNavigationActivity"
            android:label="@string/title_activity_owner_navigation"></activity>
        <activity android:name=".owner.CategoryActivity" />
        <activity android:name=".owner.Add_Product_Activity" />
        <activity
            android:name=".customer.MainActivity"
            android:label="@string/title_activity_main" />
        <activity android:name=".customer.SignUpActivity" />
        <activity android:name=".LoginActivity" />
        <activity
            android:name=".SplashScreenActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>